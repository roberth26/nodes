{
    "extends": ["tslint-config-airbnb", "tslint-react", "tslint-config-prettier"],
    "rules": {
        "no-consecutive-blank-lines": true,
        "no-unused-variable": true,
        "indent": [true, "spaces", 4],
        "import-name": false,
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-pascal-case"
        ],
        "jsx-no-lambda": false
    }
}